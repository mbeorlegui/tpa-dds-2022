{{#partial "selector"}}
    <div class="container-fluid d-flex justify-content-center border border-1 p-2">
        <a class="btn btn-primary mx-2" href="/user/calculadora/organizacion" role="button">Organizacion</a>
        <a class="btn btn-secondary mx-2" href="/user/calculadora/sectorTerritorial" role="button">Sector
            Territorial</a>
    </div>
    <div class="row">
        <div class="dropright" style="margin: 15px 0px 15px 0px;">
            <select class="form-select" data-style="btn-primary" id="entidad" name="organizacion" required>
                <option hidden value="">Seleccione una Entidad</option>
                {{#each organizaciones}}
                    <option value="{{ id }}">{{ razonSocial }}</option>
                {{/each}}
            </select>
        </div>
    </div>
{{/partial}}

{{>layoutCalculadora.hbs}}