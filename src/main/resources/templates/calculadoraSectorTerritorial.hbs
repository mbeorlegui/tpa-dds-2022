{{#partial "selector"}}
<div class="container-fluid d-flex justify-content-center border border-1 p-2">
    <a class="btn btn-secondary mx-2" href="/user/calculadoraOrganizacion" role="button">Organizacion</a>
    <a class="btn btn-primary mx-2" href="/user/calculadoraSectorTerritorial" role="button" >Sector Territorial</a>
</div>
<div class="row">
    <div class="dropright" style="margin: 15px 0px 15px 0px;">
        <select class="form-select" data-style="btn-primary" id="unidad" name="sector_territorial">
            <option value="">Seleccione Entidad:</option>
            {{#each sectores_territoriales}}
                <option value="{{ id }}">{{ nombre }}</option>
            {{/each}}
        </select>
    </div>
</div>
{{/partial}}

{{>layoutCalculadora.hbs}}